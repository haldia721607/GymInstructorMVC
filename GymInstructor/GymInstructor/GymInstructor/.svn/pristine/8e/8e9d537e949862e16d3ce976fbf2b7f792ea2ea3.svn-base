@if (ViewBag.ProfileImage != null)
{
    <img alt="image" class="rounded-circle" src="@ViewBag.ProfileImage" />
}
else
{
    <img alt="image" class="rounded-circle" src="~/Content/img/profile_small.jpg" />
}
<a data-toggle="dropdown" class="dropdown-toggle" href="#">
    <span class="block m-t-xs font-bold">@ViewBag.UserName</span>
    <span class="text-muted text-xs block">Art Director <b class="caret"></b></span>
</a>
